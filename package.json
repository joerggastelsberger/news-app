{
  "name": "news-app",
  "version": "1.0.0",
  "description": "News Application",
  "author": "Joerg Gastelsberger",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "test": "npm run test:unit",
    "test:unit": "jest --no-cache",
    "test:unit:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --no-cache --runInBand",
    "test:acceptance": "cypress run --browser chrome",
    "test:acceptance:debug": "cypress run --browser chrome --no-exit"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "nuxt": "^2.0.0",
    "slugify": "^1.4.5",
    "vuelidate": "^0.7.5"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-jest": "^24.9.0",
    "cypress": "^6.2.1",
    "flush-promises": "^1.0.2",
    "jest": "^24.9.0",
    "lodash.merge": "^4.6.2",
    "vue-jest": "^4.0.0-rc.0"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "vue-jest"
    }
  }
}
